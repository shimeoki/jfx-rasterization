<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>

<?import javafx.scene.control.Button?>

<AnchorPane fx:id="root" fx:controller="com.github.shimeoki.jfx.rasterization.demo.StaticMode" xmlns:fx="http//javafx.com/fxml">
    <HBox AnchorPane.topAnchor="0.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
        <TilePane fx:id="tilePane" hgap="20.0" vgap="20.0" HBox.hgrow="ALWAYS" style="-fx-padding: 20; -fx-border-width: 1; -fx-border-color: black; -fx-border-style: solid">
        </TilePane>
        <VBox alignment="CENTER" style="-fx-padding: 20">
            <Button fx:id="tilesBtn" text="generate"/>
        </VBox>
    </HBox>
</AnchorPane>
